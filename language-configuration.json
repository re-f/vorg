{
  "comments": {
    "lineComment": "#"
  },
  "brackets": [
    ["[", "]"],
    ["(", ")"],
    ["{", "}"]
  ],
  "autoClosingPairs": [
    ["[", "]"],
    ["(", ")"],
    ["{", "}"],
    ["\"", "\""],
    ["'", "'"],
    ["`", "`"]
  ],
  "surroundingPairs": [
    ["[", "]"],
    ["(", ")"],
    ["{", "}"],
    ["\"", "\""],
    ["'", "'"],
    ["`", "`"]
  ],
  "folding": {
    "markers": {
      "start": "^\\s*\\*+\\s+",
      "end": "^\\s*\\*+\\s+"
    }
  },
  "wordPattern": "(-?\\d*\\.\\d\\w*)|([^\\`\\~\\!\\@\\#\\%\\^\\&\\*\\(\\)\\-\\=\\+\\[\\{\\]\\}\\\\\\|\\;\\:\\'\\\"\\,\\.\\<\\>\\/\\?\\s]+)"
} 