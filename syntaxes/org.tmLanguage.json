      "patterns": [
        {
          "name": "markup.list.task.org",
          "match": "^(\\s*)(-|\\+|\\*)\\s+(\\[[ X-]\\])\\s+",
          "captures": {
            "2": {
              "name": "punctuation.definition.list.begin.org"
            },
            "3": {
              "name": "markup.list.task.checkbox.org"
            }
          }
