        {
          "name": "markup.fenced_code.block.typescript.org",
          "begin": "^\\s*#\\+(BEGIN_SRC)\\s+(typescript|ts)\\s*(.*)$",
          "end": "^\\s*#\\+(END_SRC)\\s*$",
          "beginCaptures": {
            "1": {
              "name": "keyword.control.flow.begin.org"
            },
            "2": {
              "name": "entity.name.type.language.org"
            },
            "3": {
              "name": "string.unquoted.arguments.org"
            }
          },
          "endCaptures": {
