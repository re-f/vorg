{
  "$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
  "name": "Org",
  "patterns": [
    {
      "include": "#headers"
    },
    {
      "include": "#todo-keywords"
    },
    {
      "include": "#tags"
    },
    {
      "include": "#properties"
    },
    {
      "include": "#markup"
    },
    {
      "include": "#links"
    },
    {
      "include": "#code-blocks"
    },
    {
      "include": "#quote-blocks"
    },
    {
      "include": "#example-blocks"
    },
    {
      "include": "#tables"
    },
    {
      "include": "#lists"
    },
    {
      "include": "#task-lists"
    },
    {
      "include": "#math"
    },
    {
      "include": "#timestamps"
    },
    {
      "include": "#keywords"
    },
    {
      "include": "#comments"
    },
    {
      "include": "#horizontal-rule"
    }
  ],
  "repository": {
    "headers": {
      "patterns": [
        {
          "name": "markup.heading.1",
          "match": "^(\\*{1})\\s+((?:TODO|DONE|NEXT|WAITING|CANCELLED)?\\s*)?(.*)(?:\\s+(?::[\\w]+:)+)?\\s*$",
          "captures": {
            "1": {
              "name": "punctuation.definition.heading.org"
            },
            "2": {
              "name": "keyword.other.todo.org"
            },
            "3": {
              "name": "entity.name.section.org"
            }
          }
        },
        {
          "name": "markup.heading.2",
          "match": "^(\\*{2})\\s+((?:TODO|DONE|NEXT|WAITING|CANCELLED)?\\s*)?(.*)(?:\\s+(?::[\\w]+:)+)?\\s*$",
          "captures": {
            "1": {
              "name": "punctuation.definition.heading.org"
            },
            "2": {
              "name": "keyword.other.todo.org"
            },
            "3": {
              "name": "entity.name.section.org"
            }
          }
        },
        {
          "name": "markup.heading.3",
          "match": "^(\\*{3})\\s+((?:TODO|DONE|NEXT|WAITING|CANCELLED)?\\s*)?(.*)(?:\\s+(?::[\\w]+:)+)?\\s*$",
          "captures": {
            "1": {
              "name": "punctuation.definition.heading.org"
            },
            "2": {
              "name": "keyword.other.todo.org"
            },
            "3": {
              "name": "entity.name.section.org"
            }
          }
        },
        {
          "name": "markup.heading.4",
          "match": "^(\\*{4})\\s+((?:TODO|DONE|NEXT|WAITING|CANCELLED)?\\s*)?(.*)(?:\\s+(?::[\\w]+:)+)?\\s*$",
          "captures": {
            "1": {
              "name": "punctuation.definition.heading.org"
            },
            "2": {
              "name": "keyword.other.todo.org"
            },
            "3": {
              "name": "entity.name.section.org"
            }
          }
        },
        {
          "name": "markup.heading.5",
          "match": "^(\\*{5})\\s+((?:TODO|DONE|NEXT|WAITING|CANCELLED)?\\s*)?(.*)(?:\\s+(?::[\\w]+:)+)?\\s*$",
          "captures": {
            "1": {
              "name": "punctuation.definition.heading.org"
            },
            "2": {
              "name": "keyword.other.todo.org"
            },
            "3": {
              "name": "entity.name.section.org"
            }
          }
        },
        {
          "name": "markup.heading.6",
          "match": "^(\\*{6,})\\s+((?:TODO|DONE|NEXT|WAITING|CANCELLED)?\\s*)?(.*)(?:\\s+(?::[\\w]+:)+)?\\s*$",
          "captures": {
            "1": {
              "name": "punctuation.definition.heading.org"
            },
            "2": {
              "name": "keyword.other.todo.org"
            },
            "3": {
              "name": "entity.name.section.org"
            }
          }
        }
      ]
    },
    "todo-keywords": {
      "patterns": [
        {
          "name": "keyword.other.todo.org",
          "match": "\\b(TODO|NEXT|WAITING)\\b"
        },
        {
          "name": "keyword.other.done.org",
          "match": "\\b(DONE|CANCELLED)\\b"
        }
      ]
    },
    "tags": {
      "patterns": [
        {
          "name": "entity.name.tag.org",
          "match": ":[\\w]+:"
        }
      ]
    },
    "properties": {
      "patterns": [
        {
          "name": "storage.type.property.org",
          "match": "^\\s*:([A-Z_]+):\\s*(.*)$",
          "captures": {
            "1": {
              "name": "keyword.control.property.org"
            },
            "2": {
              "name": "string.unquoted.org"
            }
          }
        }
      ]
    },
    "markup": {
      "patterns": [
        {
          "name": "markup.bold.org",
          "match": "(?<!\\w)\\*([^*\\s](?:[^*]*[^*\\s])?)\\*(?!\\w)",
          "captures": {
            "1": {
              "name": "markup.bold.org"
            }
          }
        },
        {
          "name": "markup.italic.org",
          "match": "(?<!\\w)/([^/\\s](?:[^/]*[^/\\s])?)/(?!\\w)",
          "captures": {
            "1": {
              "name": "markup.italic.org"
            }
          }
        },
        {
          "name": "markup.underline.org",
          "match": "(?<!\\w)_([^_\\s](?:[^_]*[^_\\s])?)_(?!\\w)",
          "captures": {
            "1": {
              "name": "markup.underline.org"
            }
          }
        },
        {
          "name": "markup.strikethrough.org",
          "match": "(?<!\\w)\\+([^+\\s](?:[^+]*[^+\\s])?)\\+(?!\\w)",
          "captures": {
            "1": {
              "name": "markup.strikethrough.org"
            }
          }
        },
        {
          "name": "markup.raw.inline.org",
          "match": "(?<!\\w)=([^=\\s](?:[^=]*[^=\\s])?)=(?!\\w)",
          "captures": {
            "1": {
              "name": "markup.raw.inline.org"
            }
          }
        },
        {
          "name": "markup.raw.inline.org",
          "match": "(?<!\\w)~([^~\\s](?:[^~]*[^~\\s])?)~(?!\\w)",
          "captures": {
            "1": {
              "name": "markup.raw.inline.org"
            }
          }
        }
      ]
    },
    "links": {
      "patterns": [
        {
          "name": "markup.link.with.description.org",
          "match": "\\[\\[(https?://[^\\]]+)\\]\\[([^\\]]+)\\]\\]",
          "captures": {
            "1": {
              "name": "string.other.link.url.external.org"
            },
            "2": {
              "name": "entity.name.function.link.description.org"
            }
          }
        },
        {
          "name": "markup.underline.link.external.org",
          "match": "\\[\\[(https?://[^\\]]+)\\]\\]",
          "captures": {
            "1": {
              "name": "string.other.link.url.external.org"
            }
          }
        },
        {
          "name": "markup.link.with.description.org",
          "match": "\\[\\[([^\\]]+)\\]\\[([^\\]]+)\\]\\]",
          "captures": {
            "1": {
              "name": "string.other.link.reference.org"
            },
            "2": {
              "name": "entity.name.function.link.description.org"
            }
          }
        },
        {
          "name": "markup.underline.link.internal.org",
          "match": "\\[\\[([^\\]]+)\\]\\]",
          "captures": {
            "1": {
              "name": "string.other.link.reference.org"
            }
          }
        },
        {
          "name": "markup.underline.link.bare.org",
          "match": "https?://[^\\s]+",
          "captures": {
            "0": {
              "name": "string.other.link.url.bare.org"
            }
          }
        }
      ]
    },
    "code-blocks": {
      "patterns": [
        {
          "name": "markup.fenced_code.block.python.org",
          "begin": "^\\s*#\\+(BEGIN_SRC)\\s+(python)\\s*(.*)$",
          "end": "^\\s*#\\+(END_SRC)\\s*$",
          "beginCaptures": {
            "1": {
              "name": "keyword.control.flow.begin.org"
            },
            "2": {
              "name": "entity.name.type.language.org"
            },
            "3": {
              "name": "string.unquoted.arguments.org"
            }
          },
          "endCaptures": {
            "1": {
              "name": "keyword.control.flow.end.org"
            }
          },
          "patterns": [
            {
              "include": "source.python"
            }
          ]
        },
        {
          "name": "markup.fenced_code.block.javascript.org",
          "begin": "^\\s*#\\+(BEGIN_SRC)\\s+(javascript|js)\\s*(.*)$",
          "end": "^\\s*#\\+(END_SRC)\\s*$",
          "beginCaptures": {
            "1": {
              "name": "keyword.control.flow.begin.org"
            },
            "2": {
              "name": "entity.name.type.language.org"
            },
            "3": {
              "name": "string.unquoted.arguments.org"
            }
          },
          "endCaptures": {
