      "patterns": [
        {
          "name": "markup.table.org",
          "match": "^\\s*(\\|.*)$",
          "captures": {
            "1": {
              "name": "markup.table.row.org",
              "patterns": [
                {
                  "name": "punctuation.separator.table.org",
                  "match": "\\|"
                },
                {
                  "name": "markup.table.cell.org",
                  "match": "[^|]+"
                }
              ]
